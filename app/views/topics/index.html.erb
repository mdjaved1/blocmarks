<h1> Topics <%= link_to "New Topic", new_topic_path, class: 'btn btn-success' %> </h1>

  <div class="row">
    <div class="col-md-8">
      <% @topics.each do |topic| %>
      <div class="media">
        <div class="media-body">
          <h4 class="media-heading">
             <%= link_to topic.title, topic %>
             <div class="bookmark_box">
               <p> </p>
             </div>
          </h4>
        </div>
      </div>
      <% end %>
    </div>
  </div>